                <label for="python">Python</label><br />
            </div>

            <div class="col col-md-3 col-sm-6">
                <input type="checkbox" id="r" name="language" value="r" />
                <label for="r">R</label><br />
            </div>

            <div class="col col-md-3 col-sm-6">
                <input type="checkbox" id="roff" name="language" value="roff" />
                <label for="roff">Roff</label><br />
            </div>

            <div class="col col-md-3 col-sm-6">
                <input type="checkbox" id="ruby" name="language" value="ruby" />
                <label for="ruby">Ruby</label><br />
            </div>
        </div>

        <div class="row border border-primary">
            <div class="col col-md-3 col-sm-6">
                <input type="checkbox" id="rust" name="language" value="rust" />
                <label for="rust">Rust</label><br />
            </div>

            <div class="col col-md-3 col-sm-6">
                <input
                    type="checkbox"
                    id="scala"
                    name="language"
                    value="scala"
                />
                <label for="scala">Scala</label><br />
            </div>

            <div class="col col-md-3 col-sm-6">
                <input type="checkbox" id="scss" name="language" value="scss" />
                <label for="scss">SCSS</label><br />
            </div>

            <div class="col col-md-3 col-sm-6">
                <input
                    type="checkbox"
                    id="shell"
                    name="language"
                    value="shell"
                />
                <label for="shell">Shell</label><br />
